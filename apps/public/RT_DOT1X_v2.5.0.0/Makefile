
ifndef CFLAGS
CFLAGS = -O2 -Wall -g -DDBG
endif

# If you want to support multiple radius server for multiple bssid, add following line
CFLAGS +=  -DMULTIPLE_RADIUS

ifneq ($(TC_CFLAGS), )
        CFLAGS+=$(TC_CFLAGS)
endif

OBJS =	rtdot1x.o eloop.o eapol_sm.o radius.o md5.o  \
	config.o ieee802_1x.o  \
	sta_info.o   radius_client.o

all: rtdot1xd

rtdot1xd: $(OBJS)
	$(CC) -o rtdot1xd $(OBJS)
	#cp rtdot1xd /tftpboot
	$(STRIP) --strip-unneeded rtdot1xd

clean:
	rm -f core *~ *.o rtdot1xd *.d

-include $(OBJS:%.o=%.d)
